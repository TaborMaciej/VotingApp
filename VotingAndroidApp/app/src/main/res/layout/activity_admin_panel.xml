<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminPanelActivity">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Panel członka komisji "
        android:textSize="118sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/EndVoteBtn"
        android:layout_width="520dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:lines="2"
        android:padding="8dp"
        android:text="Zakończ głosowanie"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.33" />

    <Button
        android:id="@+id/SyncBtn"
        android:layout_width="520dp"
        android:layout_height="0dp"
        android:layout_marginEnd="64dp"
        android:lines="2"
        android:padding="8dp"
        android:text="Importuj dane"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/EndVoteBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/EndVoteBtn" />

    <Button
        android:id="@+id/backBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="256dp"
        android:layout_marginTop="64dp"
        android:padding="24dp"
        android:text="Powrót"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EndVoteBtn" />
</androidx.constraintlayout.widget.ConstraintLayout>